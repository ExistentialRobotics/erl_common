@PACKAGE_INIT@

if (NOT COMMAND erl_project_setup)
    find_package(erl_cmake_tools REQUIRED)
endif ()

set(ERL_IGNORE_CONDA_LIBRARIES @ERL_IGNORE_CONDA_LIBRARIES@)

set(ERL_USE_LAPACK @ERL_USE_LAPACK@)
set(ERL_USE_LAPACK_STRICT @ERL_USE_LAPACK_STRICT@)
set(ERL_USE_INTEL_MKL @ERL_USE_INTEL_MKL@)
set(ERL_USE_AOCL @ERL_USE_AOCL@)
set(ERL_USE_SINGLE_THREADED_BLAS @ERL_USE_SINGLE_THREADED_BLAS@)

set(ERL_USE_TRACY @ERL_USE_TRACY@)
set(ERL_USE_PANGOLIN @ERL_USE_PANGOLIN@)
set(ERL_USE_PLPLOT @ERL_USE_PLPLOT@)

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@_dependencies.cmake")
include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

# name alias
add_library(@PROJECT_NAME@::@PROJECT_NAME@ ALIAS @PROJECT_NAME@)

check_required_components("@PROJECT_NAME@")

# export variables
erl_mark_project_found(PROJECT_NAME @PROJECT_NAME@)
